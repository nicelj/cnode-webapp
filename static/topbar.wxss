.index-container{
  background: #DBDBDB;
  height: 50px; 
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-content: center;
   /* position: fixed;  */
  z-index: 1;
}
.title{
  line-height: 50px;
  display: flex;
  justify-content: space-between;
  flex-flow: row nowrap;
  align-items: center;
  width:100%;
  padding: 0 10px;
}
.title text{
  color: #808080;
  letter-spacing: 2px;
}
.index-img{
  width:30px;
  height:30px;
  vertical-align: middle;
}